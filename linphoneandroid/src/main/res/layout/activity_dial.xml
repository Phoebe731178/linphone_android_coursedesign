<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
    <LinearLayout
            android:id="@+id/container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#7adfb8"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:gravity="center_vertical"
            android:orientation="vertical"
            android:paddingBottom="5dp"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:paddingTop="5dp"
            tools:ignore="MergeRootFrame" >
        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1.5"
                android:orientation="horizontal">
            <EditText
                    android:id="@+id/editText1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right"
                    android:layout_weight="1.5"
                    android:textSize="40sp"/>
            <ImageButton
                    android:id="@+id/deleteNumber"

                    android:layout_width="51dp"
                    android:layout_height="50dp"
                    android:background="@drawable/number_ff_delete"/>

        </LinearLayout>


        <TableLayout
                android:layout_width="fill_parent"
                android:layout_height="0dp"
                android:layout_weight="8">

            <TableRow
                    android:layout_marginTop="54dp"
                    android:id="@+id/tableRow1"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.75">

                <ImageButton
                        android:id="@+id/imageButton1"
                        android:layout_marginRight="80dp"

                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/number_ff_1"
                        android:onClick="telNumber"
                        android:tag="1"/>
                <ImageButton
                        android:id="@+id/imageButton2"
                        android:layout_marginRight="80dp"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/number_ff_2"
                        android:onClick="telNumber"
                        android:tag="2"/>

                <ImageButton
                        android:id="@+id/imageButton3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/number_ff_3"
                        android:onClick="telNumber"
                        android:tag="3"/>

            </TableRow>

            <TableRow
                    android:layout_marginTop="54dp"
                    android:id="@+id/TableRow01"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.9">

                <ImageButton
                        android:id="@+id/ImageButton01"
                        android:layout_marginRight="80dp"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/number_ff_4"
                        android:onClick="telNumber"
                        android:tag="4" />

                <ImageButton
                        android:id="@+id/ImageButton02"
                        android:layout_marginRight="80dp"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/number_ff_5"
                        android:onClick="telNumber"
                        android:tag="5" />

                <ImageButton
                        android:id="@+id/ImageButton03"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/number_ff_6"
                        android:onClick="telNumber"
                        android:tag="6" />

            </TableRow>

            <TableRow
                    android:layout_marginTop="54dp"
                    android:id="@+id/TableRow02"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1.3" >

                <ImageButton
                        android:id="@+id/ImageButton04"
                        android:layout_marginRight="80dp"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/number_ff_7"


                        android:padding="10dp"
                        android:onClick="telNumber"
                        android:tag="7" />

                <ImageButton
                        android:id="@+id/ImageButton05"
                        android:layout_marginRight="80dp"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/number_ff_8"
                        android:onClick="telNumber"
                        android:tag="8" />

                <ImageButton
                        android:id="@+id/ImageButton06"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/number_ff_9"
                        android:onClick="telNumber"
                        android:tag="9" />

            </TableRow>

            <TableRow
                    android:layout_marginTop="54dp"
                    android:id="@+id/TableRow03"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    android:layout_weight="1.2">

                <ImageButton
                        android:id="@+id/ImageButton07"
                        android:layout_marginRight="80dp"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/number_ff_"
                        android:onClick="telNumber"
                        android:tag="*" />

                <ImageButton
                        android:id="@+id/ImageButton08"
                        android:layout_marginRight="80dp"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/number_ff_0"
                        android:onClick="telNumber"
                        android:tag="0" />

                <ImageButton
                        android:id="@+id/ImageButton09"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/jinhao_ff"
                        android:onClick="telNumber"
                        android:tag="#" />

            </TableRow>
        </TableLayout>

        <LinearLayout
                android:layout_marginTop="40dp"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1.75"
                android:orientation="vertical">

            <ImageButton
                    android:id="@+id/dialCall"
                    android:layout_width="100dp"
                    android:layout_height="80dp"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/answer2"
                    />

        </LinearLayout>


    </LinearLayout>


</RelativeLayout>
